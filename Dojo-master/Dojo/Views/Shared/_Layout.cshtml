<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Master Stefanos Dojo</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link href="~/Content/NewContent/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link href="~/Content/NewContent/dist/css/AdminLTE.min.css" rel="stylesheet" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/NewContent/dist/css/skins/_all-skins.min.css">
    <!-- iCheck -->
    <link href="~/Content/NewContent/plugins/iCheck/flat/blue.css" rel="stylesheet" />
    <!-- Morris chart -->
    <link rel="stylesheet" href="~/Content/NewContent/plugins/morris/morris.css">
    <!-- jvectormap -->
    <link rel="stylesheet" href="~/Content/NewContent/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
    <!-- Date Picker -->
    <link rel="stylesheet" href="~/Content/NewContent/plugins/datepicker/datepicker3.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Content/NewContent/plugins/daterangepicker/daterangepicker.css">
    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/Content/NewContent/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <link href="~/Content/NewContent/chosen/chosen.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue fixed sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="@Url.Action("Index", "Home")" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>P</b>VF</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>DOJO</b><i class="fa  fa-spinner"></i></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <div id="site_content">
                        @* @Html.Partial("_Search")*@
                    </div>
                    @{ if (ViewBag.CurrentFinancialDate != null)
                        {
                            <span>Current Financial Date: @ViewBag.CurrentFinancialDate</span>
                        }
                    }

                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">MAIN NAVIGATION</li>
                    @{
                        {
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-map"></i> <span>Students</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Students")"><i class="fa fa-plus"></i> Register New Student</a></li>
                                    <li><a href="@Url.Action("Index", "Students")"><i class="fa fa-eye"></i> View Students</a></li>
                                </ul>
                            </li>


                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-user"></i> <span>Attendance</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Attendances")"><i class="fa fa-user-plus"></i> Take Attendance</a></li>
                                    <li><a href="@Url.Action("Index", "Attendances")"><i class="fa fa-eye"></i> View Attendance Report</a></li>
                                </ul>
                            </li>

                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-users"></i> <span>Classes</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Classes")"><i class="fa fa-user-plus"></i> Add New Class</a></li>
                                    <li><a href="@Url.Action("Index", "Classes")"><i class="fa fa-eye"></i> View Classes</a></li>
                                </ul>
                            </li>

                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-tag"></i> <span>Inventory</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Products")"><i class="fa fa-plus-square"></i> Add    Item</a></li>
                                    <li><a href="@Url.Action("Index", "Products")"><i class="fa fa-eye"></i> View Inventory</a></li>
                                    <li><a href="@Url.Action("Create", "ProductPayments")"><i class="fa fa-plus-square"></i> New  Purchase </a></li>
                                    <li><a href="@Url.Action("Index", "ProductPayments")"><i class="fa fa-plus-square"></i>  Purchase Log</a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-graduation-cap"></i> <span>Memberships</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Memberships")"><i class="fa fa-plus-square"></i> Create Membership Type</a></li>
                                    <li><a href="@Url.Action("Index", "Memberships")"><i class="fa fa-eye"></i> View Membership Types</a></li>
                                    <li><a href="@Url.Action("Create", "MembershipPayments")"><i class="fa fa-plus-square"></i> New Subscription </a></li>
                                    <li><a href="@Url.Action("Index", "MembershipPayments")"><i class="fa fa-plus-square"></i> View Subscriptions </a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-bell-o"></i> <span>Ranks</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Ranks")"><i class="fa fa-plus-square"></i> Add Ranks</a></li>
                                    <li><a href="@Url.Action("Index", "Ranks")"><i class="fa fa-eye"></i> View Ranks</a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-truck"></i> <span>Test</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Tests")"><i class="fa fa-plus-square"></i> Create Test</a></li>
                                    <li><a href="@Url.Action("Index", "Tests")"><i class="fa fa-eye"></i> View Tests</a></li>
                                    <li><a href="@Url.Action("Create", "TestPayments")"><i class="fa fa-plus-square"></i> New Purchase </a></li>
                                    <li><a href="@Url.Action("Index", "TestPayments")"><i class="fa fa-plus-square"></i> View Purchases </a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-user"></i> <span>Parents</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Parents")"><i class="fa fa-plus-square"></i> Add Parents</a></li>
                                    <li><a href="@Url.Action("Index", "Parents")"><i class="fa fa-eye"></i> View Parents</a></li>
                                </ul>
                            </li>
                            <li class="treeview">
                                <a href="#">
                                    <i class="fa fa-industry"></i> <span>Certificate</span>
                                    <span class="pull-right-container">
                                        <i class="fa fa-angle-left pull-right"></i>
                                    </span>
                                </a>
                                <ul class="treeview-menu">
                                    <li><a href="@Url.Action("Create", "Certificates")"><i class="fa fa-plus-square"></i> Add Certificate</a></li>
                                    <li><a href="@Url.Action("Index", "Certificates")"><i class="fa fa-eye"></i> View Certificate</a></li>
                                    <li><a href="@Url.Action("Create", "HAS_CERTIFICATE")"><i class="fa fa-plus-square"></i> Issue Certificate</a></li>
                                    <li><a href="@Url.Action("Index", "HAS_CERTIFICATE")"><i class="fa fa-plus-square"></i> View Issued Certificates </a></li>
                                </ul>
                            </li>

                        }

                    }

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <!-- Main content -->
            @RenderBody()

            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.5.1
            </div>

            <strong>Copyright &copy; @DateTime.Now.Year <a href="~/Views/Home/About.cshtml">Dojo-APP</a>.</strong> All rights Reserved.
        </footer>

    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.3 -->
    <script src="~/Content/NewContent/plugins/jQuery/jquery-2.2.3.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Content/NewContent/bootstrap/js/bootstrap.min.js"></script>
    <!-- Morris.js charts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="~/Content/NewContent/plugins/morris/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Content/NewContent/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- jvectormap -->
    <script src="~/Content/NewContent/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/NewContent/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/NewContent/plugins/knob/jquery.knob.js"></script>
    <!-- daterangepicker -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <script src="~/Content/NewContent/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- datepicker -->
    <script src="~/Content/NewContent/plugins/datepicker/bootstrap-datepicker.js"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <script src="~/Content/NewContent/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/NewContent/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <!-- FastClick -->
    <script src="~/Content/NewContent/plugins/fastclick/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/NewContent/dist/js/app.min.js"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="~/Content/NewContent/dist/js/pages/dashboard.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/NewContent/dist/js/demo.js"></script>
    <script src="~/Content/NewContent/chosen/chosen.jquery.min.js"></script>
    <script src="~/Scripts/app.js"></script>
    <script>

        $('.chosen-select').chosen();

        $(function () {
            $("#example1").DataTable();
            $('#example2').DataTable({
                "paging": true,
                "lengthChange": false,
                "searching": true,
                "ordering": true,
                "info": true,
                "autoWidth": false
            });
        });
    </script>
</body>
</html>
